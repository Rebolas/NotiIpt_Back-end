@model IEnumerable<NotiIpt.Models.Dados>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Temperatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Humidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoDia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Luz)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Temperatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Humidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoDia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Luz)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DataHora">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DataHora">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DataHora">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
